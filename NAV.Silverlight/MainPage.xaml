<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:NAV_Silverlight_Views="clr-namespace:NAV.Silverlight.Views" xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" x:Class="NAV.Silverlight.MainPage"
	xmlns:xaml="clr-namespace:NAV.Silverlight.Xaml"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400"
	DataContext="{Binding Path=Main, Source={StaticResource Locator}}">

    <Grid x:Name="LayoutRoot" Background="White">

		<sdk:Frame
				   xaml:NavigationBinder.UriProperty="{Binding NextView}" Content="" />

	</Grid>
</UserControl>
